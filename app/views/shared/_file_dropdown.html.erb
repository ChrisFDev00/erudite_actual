<ul class="list-none">
  <% if obj.files.empty? %>
      <span>
          <%=  "No files found. Please contact your teacher."  %>
      </span>
  <%  else  %>
  <%  obj.files.each do |file|  %>
  <div class="collapse bg-base-100 border-base-300 border">
    <input type="checkbox"/>
    <div class="collapse-title font-semibold"><%=  file.filename  %></div>
    <div class="collapse-content text-sm">
      <div id="pdf-container" data-pdf-url="<%=  rails_blob_url(file)  %>"></div>
    </div>
  </div>
  <%  end  %>
  <%  end %>
</ul>
